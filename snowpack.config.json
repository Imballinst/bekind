{
  "mount": {
    "public": "/",
    "src": "/_dist_"
  },
  "extends": "@snowpack/app-scripts-vue",
  "plugins": [
    "@snowpack/plugin-dotenv",
    "@snowpack/plugin-vue",
    [
      "@snowpack/plugin-build-script",
      { "cmd": "postcss", "input": [".css"], "output": [".css"] }
    ],
    ["@snowpack/plugin-webpack"]
  ],
  "proxy": {}
}
